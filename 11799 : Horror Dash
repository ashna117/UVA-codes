#include<stdio.h>

int main() {

int c[10000],T,N,cases=0,i,max;
scanf("%d", &T);
while(T--)
{
    cases++;
    scanf("%d", &N);
    for(i=0; i<N; i++)
    {
        scanf("%d", &c[i]);
    }
    max = c[0];
    for(i=0; i<N; i++)
    {
        if(c[i]>max)
            max = c[i];
    }
    printf("Case %d: %d\n",cases,max);
}



 return 0;
}
